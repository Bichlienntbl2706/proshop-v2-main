{
  "name": "proshop2",
  "version": "2.0.0",
  "type": "module",
  "description": "Ecommerce application built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend",
    "generate-toc": "markdown-toc -i readme.md"
  },
  "author": "Brad Traversy",
  "license": "MIT",
  "dependencies": {
    "@payos/node": "^1.0.6",
    "await": "^0.2.6",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "config": "^1.31.0",
    "cookie-parser": "^1.4.6",
    "crypto-browserify": "^3.12.0",
    "dateformat": "^2.2.0",
    "debug": "^3.2.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jade": "^1.9.2",
    "jquery": "^3.4.0",
    "jsonwebtoken": "^9.0.0",
    "md5": "^2.2.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.4",
    "mongoose": "^7.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.9",
    "qs": "^6.7.0",
    "request": "^2.88.2",
    "serve-favicon": "^2.4.5",
    "set-tz": "^0.2.0",
    "sha256": "^0.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "concurrently": "^7.6.0",
    "markdown-toc": "^1.2.0",
    "nodemon": "^3.0.3"
  }
}
